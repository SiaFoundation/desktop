{
  "name": "desktop",
  "version": "0.0.0",
  "description": "The desktop apps",
  "author": {
    "name": "The Sia Foundation",
    "email": "hello@sia.tech"
  },
  "scripts": {
    "release": "changeset publish",
    "icons": "yarn icons:renterd && yarn icons:hostd",
    "icons:renterd": "electron-icon-builder --input=renterd/appicon.png --output=renterd",
    "icons:hostd": "electron-icon-builder --input=hostd/appicon.png --output=hostd"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "hostd",
      "renterd"
    ]
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "electron-icon-builder": "^2.0.1"
  },
  "packageManager": "yarn@4.0.2"
}
