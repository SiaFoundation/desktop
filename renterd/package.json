{
  "name": "renterd",
  "version": "0.19.0",
  "license": "MIT",
  "private": true,
  "description": "The renterd desktop app",
  "author": {
    "name": "The Sia Foundation",
    "email": "hello@sia.tech"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SiaFoundation/desktop.git"
  },
  "main": "dist/main/index.js",
  "scripts": {
    "clean": "rimraf daemon/bin out dist renderer/.next",
    "fresh": "npm run clean && rimraf node_modules",
    "dev": "npm run build:main && electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "next build renderer",
    "build:main": "tsc -p main",
    "build:scripts": "tsc -p scripts",
    "lint": "eslint main && eslint scripts",
    "download:binary": "npm run build:scripts && node dist/scripts/download.js",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./main/tsconfig.json",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "icons": "electron-icon-builder --flatten --input=assets/appicon.png --output=temp && npm run icons:copy && npm run icons:cleanup",
    "icons:copy:linux": "cp temp/icons/512x512.png assets/icons/icon.png",
    "icons:copy:win": "cp temp/icons/icon.ico assets/icons/icon.ico",
    "icons:copy:mac": "cp temp/icons/icon.icns assets/icons/icon.icns",
    "icons:copy": "npm run icons:copy:linux && npm run icons:copy:win && npm run icons:copy:mac",
    "icons:cleanup": "rimraf temp"
  },
  "dependencies": {
    "@electron-forge/maker-dmg": "^7.4.0",
    "@fastify/deepmerge": "^1.3.0",
    "@octokit/rest": "^20.0.2",
    "@siafoundation/design-system": "^4.18.0",
    "@siafoundation/react-core": "^2.0.0",
    "@siafoundation/react-icons": "^0.3.0",
    "@technically/lodash": "^4.17.0",
    "adm-zip": "^0.5.10",
    "app-root-path": "^3.1.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "bip39": "^3.1.0",
    "class-variance-authority": "^0.7.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "swr": "^2.1.1",
    "tailwind-scrollbar": "^2.0.1",
    "update-electron-app": "^3.0.0",
    "use-local-storage-state": "^18.3.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@electron-forge/publisher-s3": "^7.2.0",
    "@types/adm-zip": "^0.5.5",
    "@types/appdmg": "^0.5.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "18.15.11",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "autoprefixer": "^10.4.16",
    "electron": "^30.0.5",
    "eslint": "^8.0.0",
    "eslint-config-next": "13.3.0",
    "globals": "^15.2.0",
    "next": "14.2.14",
    "postcss": "^8.4.31",
    "rimraf": "^3.0.2",
    "typescript": "5.0.3",
    "typescript-eslint": "^7.9.0"
  }
}
